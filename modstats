function getUser(user)
{
	return $.grep(json.total_monthly, function(e){ return e.username == user; });
}

$('#orderByUsername a[href^="/user/"]').each(function(index, value){ 
	var mod = $(value).text();
	console.log(mod);
	var modUser = getUser(mod)[0];
	console.log(modUser);
	if(modUser != undefined)
	{
		console.log("WE MADE IT AT " + modUser.total)
		$(value).append("</a><div stype='color:red'>");
		$(value).append(modUser.total);
		$(value).append("</div><a>");
		//$(value).append(" <div class='REDSHIT'>" + modUser.total + "</div>");
	}
})
